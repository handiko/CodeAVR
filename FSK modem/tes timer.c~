#include <mega16.h>

void main(void)
{       PORTD=0b00000000;
        DDRD =0b11111111;

        TCCR1A=0xA1;
        TCCR1B=0x04;
        //TCCR1B=0x02;


        ACSR=0x80;
        SFIOR=0x00;

        while (1)
        {       OCR1AL=150;
        };
}
