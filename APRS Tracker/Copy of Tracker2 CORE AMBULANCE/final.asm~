;*           			UNIVERSITAS GADJAH MADA
;*
;* Chip type           	: 	ATtiny2313
;* Program type        	: 	Application
;* Clock frequency     	: 	11.059200 MHz
;* Memory model        	: 	Tiny
;* External SRAM size  	: 	0
;* Data Stack size     	: 	32
;*
;* File			:	final.c
;*
;* Fungsi - fungsi	: 	void set_dac(char value)
;* 				void set_nada(char i_nada)
;* 				void kirim_karakter(unsigned char input)
;* 				void kirim_paket(void)
;* 				void ubah_nada(void)
;* 				void hitung_crc(char in_crc)
;* 				void kirim_crc(void)
;* 				void ekstrak_gps(void)
;* 				void init_usart(void)
;* 				void clear_usart(void)
;* 				void timer_detik(char detik)
;*
;* Variabel global	:	char rx_buffer[RX_BUFFER_SIZE]
;* 				bit rx_buffer_overflow
;*                               flash char matrix[ ]
;*				flash char data_1[ ]
;*				char posisi_lat[ ]
;*				char posisi_long[ ]
;*				flash char data_extension[ ]
;*				flash char komentar[ ]
;*				char xcount
;*				bit nada
;*				static char bit_stuff
;*				unsigned short crc;
;*
;* Konstanta custom	:	_1200
;* 				_2200
;* 				CONST_1200
;* 				CONST_2200
;* 				GAP_TIME_
;*				INITIAL_TIME_C
;*				FWD_TIME_C
;*				FLAG_
;*				CONTROL_FIELD_
;*				PROTOCOL_ID_
;*				TD_POSISI_
;*				SYM_TAB_OVL_
;*				SYM_CODE_
;*
;* Chip I/O		:	TX_NOW  PIND.3
;* 				PTT     PORTB.3
;* 				DAC_0   PORTB.7
;* 				DAC_1   PORTB.6
;* 				DAC_2   PORTB.5
;* 				DAC_3   PORTB.4
;* 				L_BUSY	PORTD.5
;* 				L_STBY  PORTD.4
;*
;***************************************************************************************/
;
;// header firmware
;#include <tiny2313.h>
	#ifndef __SLEEP_DEFINED__
	#define __SLEEP_DEFINED__
	.EQU __se_bit=0x20
	.EQU __sm_mask=0x50
	.EQU __sm_powerdown=0x10
	.EQU __sm_standby=0x40
	.SET power_ctrl_reg=mcucr
	#endif
;#include <stdio.h>
;
;/***************************************************************************************
;* ABSTRAKSI		:	Perintah - perintah di bawah ini digunakan untuk pendeklarasian
;*				konstanta - konstanta, fungsi - fungsi, dan variabel yang
;*				dipakai dalam pelaksanaan data receiving dari PIND.0 (RX
;*				USART)
;*
;* PARAMETER USART	:	RX only, RX interrupt, 4800 baudrate, 8 data bits, No
;*				Parity, 1 stop bit
;*
;*/
;
;// definisi konstanta register USART RXB8
;#ifndef RXB8
;#define RXB8 1
;#endif
;
;// definisi konstanta register USART UPE
;#ifndef	UPE
;#define UPE 2
;#endif
;
;// definisi konstanta register USART OVR
;#ifndef	OVR
;#define OVR 3
;#endif
;
;// definisi konstanta register USART FE
;#ifndef	FE
;#define FE 4
;#endif
;
;// definisi konstanta register USART UDRE
;#ifndef	UDRE
;#define UDRE 5
;#endif
;
;// definisi konstanta register USART RXC
;#ifndef	RXC
;#define RXC 7
;#endif
;
;// definisi konstanta USART default condition
;#define FRAMING_ERROR (1<<FE)
;#define PARITY_ERROR (1<<UPE)
;#define DATA_OVERRUN (1<<OVR)
;#define DATA_REGISTER_EMPTY (1<<UDRE)
;#define RX_COMPLETE (1<<RXC)
;#define RX_BUFFER_SIZE 8
;
;// variabel global USART
;char rx_buffer[RX_BUFFER_SIZE];
;bit rx_buffer_overflow;
;
;// deklarasi variabel USART terkondisi
;#if RX_BUFFER_SIZE<256
;	unsigned char rx_wr_index,rx_rd_index,rx_counter;
;	#else
;	unsigned int rx_wr_index,rx_rd_index,rx_counter;
;#endif
;
;// interupsi RX USART
;interrupt [USART_RXC] void usart_rx_isr(void)
; 0000 009C {

	.CSEG
_usart_rx_isr:
	ST   -Y,R30
	IN   R30,SREG
	ST   -Y,R30
; 0000 009D 	char status,data;
; 0000 009E 
; 0000 009F 	status=UCSRA;
	RCALL __SAVELOCR2
;	status -> R17
;	data -> R16
	IN   R17,11
; 0000 00A0 	data=UDR;
	IN   R16,12
; 0000 00A1 	if ((status & (FRAMING_ERROR | PARITY_ERROR | DATA_OVERRUN))==0)
	MOV  R30,R17
	ANDI R30,LOW(0x1C)
	BRNE _0x3
; 0000 00A2    	{
; 0000 00A3         	rx_buffer[rx_wr_index]=data;
	MOV  R30,R3
	SUBI R30,-LOW(_rx_buffer)
	ST   Z,R16
; 0000 00A4    		if (++rx_wr_index == RX_BUFFER_SIZE) rx_wr_index=0;
	INC  R3
	LDI  R30,LOW(8)
	CP   R30,R3
	BRNE _0x4
	CLR  R3
; 0000 00A5    		if (++rx_counter == RX_BUFFER_SIZE)
_0x4:
	INC  R5
	LDI  R30,LOW(8)
	CP   R30,R5
	BRNE _0x5
; 0000 00A6       		{	rx_counter=0;
	CLR  R5
; 0000 00A7       			rx_buffer_overflow=1;
	SBI  0x13,0
; 0000 00A8       		};
_0x5:
; 0000 00A9    	};
_0x3:
; 0000 00AA }
	RCALL __LOADLOCR2P
	LD   R30,Y+
	OUT  SREG,R30
	LD   R30,Y+
	RETI
;
;// fungsi getchar USART
;#ifndef _DEBUG_TERMINAL_IO_
;#define _ALTERNATE_GETCHAR_
;#pragma used+
;	char getchar(void)
; 0000 00B1 	{
_getchar:
; 0000 00B2         	char data;
; 0000 00B3 
; 0000 00B4 		while (rx_counter==0);
	ST   -Y,R17
;	data -> R17
_0x8:
	TST  R5
	BREQ _0x8
; 0000 00B5 		data=rx_buffer[rx_rd_index];
	LDI  R26,LOW(_rx_buffer)
	ADD  R26,R2
	LD   R17,X
; 0000 00B6 		if (++rx_rd_index == RX_BUFFER_SIZE) rx_rd_index=0;
	INC  R2
	LDI  R30,LOW(8)
	CP   R30,R2
	BRNE _0xB
	CLR  R2
; 0000 00B7 		#asm("cli")
_0xB:
	cli
; 0000 00B8 		--rx_counter;
	DEC  R5
; 0000 00B9 		#asm("sei")
	sei
; 0000 00BA 		return data;
	MOV  R30,R17
	RJMP _0x2060003
; 0000 00BB 	}
;#pragma used-
;#endif
;// Akhir dari USART
;
;
;/***************************************************************************************
;*
;*	DEFINISI KONSTANTA - KONSTANTA CUSTOM
;*
;*/
;// definisi konstanta kondisi tone yang dikirimkan
;#define _1200		0
;#define _2200		1
;
;// definisi konstanta waktu de-sampling (rekonstruksi) diskrit gelombang sinus untuk tone
;	// 1200Hz dan 2200Hz dalam microsecond (us). Silahkan fine tune konstanta ini untuk
;        // adjusting baudrate dan cek hasilnya dengan menginputkan audio dari hardware APRS
;        // pada PC / Laptop lalu cek hasil tone dan baudrate dengan Cool Edit pro pada
;        // tampilan waveform atau spektral.
;
;        // Konstanta untuk kompilasi dalam mode optimasi ukuran
;#ifdef	_OPTIMIZE_SIZE_
;	#define CONST_1200      46
;	#define CONST_2200      22
;
;        // Konstanta untuk kompilasi dalam mode optimasi kecepatan
;#else
;	#define CONST_1200      50
;	#define CONST_2200      25
;#endif
;
;// waktu jeda antara transmisi data dalam detik (s)
;#define GAP_TIME_	18
;
;// konstanta waktu timer1_ovf_isr awal
;#define INITIAL_TIME_C	5
;
;// konstanta waktu timer1_ovf_isr selanjutnya
;#define FWD_TIME_C	2
;
;// konstanta waktu opening flag
;#define TX_DELAY_	40
;
;// definisi konstanta karakter Flag
;#define FLAG_		0x7E
;
;// definisi konstanta karakter Control Field
;#define	CONTROL_FIELD_	0x03
;
;// definisi konstanta karakter PID
;#define PROTOCOL_ID_	0xF0
;
;// definisi konstanta karakter Tipe Data
;#define TD_POSISI_	'='
;
;// definisi konstanta karakter simbol tabel dan overlay
;#define SYM_TAB_OVL_	'/'
;
;// definisi konstanta karakter simbol station (Seven)
;#define SYM_CODE_	'7'
;
;// konstanta waktu closing flag
;#define TX_TAIL_	2
;
;//	AKHIR DARI DEFINISI KONSTANTA - KONSTANTA CUSTOM
;
;
;/**************************************************************************************/
;
;// header firmware
;#include <delay.h>
;#include <stdarg.h>
;
;/***************************************************************************************
;*
;*	DEFINISI INPUT - OUTPUT ATTINY2313
;*
;*/
;// definisi input TX manual (request interupsi eksternal) INT1
;#define TX_NOW  PIND.3
;
;// definisi output LED TX dan transistor sebagai switch TX (Hi = TX, Lo = waiting)
;#define PTT     PORTB.3
;
;// definisi output tegangan DAC ladder resistor sebagai generator sinusoid ( DAC_0 = LSB,
;	// DAC_3 = MSB )
;#define DAC_0   PORTB.7
;#define DAC_1   PORTB.6
;#define DAC_2   PORTB.5
;#define DAC_3   PORTB.4
;
;// definisi output LED saat terima dan ekstrak data GPS (Hi = parsing, Lo = waiting)
;#define L_BUSY	PORTD.5
;
;// definisi output LED saat menunggu interupsi (Hi = waiting, Lo = ada proses)
;#define L_STBY  PORTD.4
;
;//	AKHIR DARI DEFINISI INPUT - OUTPUT ATTINY2313
;
;
;/***************************************************************************************
;*
;*	DEKLARASI PROTOTYPE FUNGSI - FUNGSI
;*
;*/
;void set_dac(char value);
;void set_nada(char i_nada);
;void kirim_karakter(unsigned char input);
;void kirim_paket(void);
;void ubah_nada(void);
;void hitung_crc(char in_crc);
;void kirim_crc(void);
;void ekstrak_gps(void);
;void init_usart(void);
;void clear_usart(void);
;void timer_detik(char detik);
;
;//	AKHIR DARI DEKLARASI PROTOTYPE FUNGSI - FUNGSI
;
;
;/***************************************************************************************
;*
;*	DEKLARASI VARIABEL GLOBAL
;*
;*/
;
;// variabel penyimpan nilai rekonstruksi diskrit gelombang sinusoid (matrix 16 ele.)
;flash char matrix[16] = {7,10,13,14,15,14,13,10,7,5,2,1,0,1,2,5};
;
;// variabel penyimpan data adresses
;flash char data_1[28] =
;{
;	// destination field, tergeser kiri 1 bit
;        ('A'<<1),('P'<<1),('Z'<<1),('T'<<1),('2'<<1),('3'<<1),0b11100000,
;
;        // source field, tergeser kiri 1 bit
;	('3'<<1),('6'<<1),('7'<<1),('0'<<1),('9'<<1),(' '<<1),('7'<<1),
;
;        // first path, tergeser kiri 1 bit
;        ('W'<<1),('I'<<1),('D'<<1),('E'<<1),('1'<<1),(' '<<1),('1'<<1),
;
;        // last path, tergeser kiri 1 bit
;        ('W'<<1),('I'<<1),('D'<<1),('E'<<1),('2'<<1),(' '<<1),('2'<<1)+1
;};
;
;// variabel penyimpan data awal posisi lintang dan update data gps
;char posisi_lat[8] =
;{
;	// latitude
;        '0','7','4','5','.','3','1','S'
;};

	.DSEG
;
;// variabel penyimpan data awal posisi bujur dan update data gps
;char posisi_long[9] =
;{
;	// longitude
;        '1','1','0','2','2','.','5','2','E'
;};
;
;// variabel penyimpan data extensi tipe PHGD
;flash char data_extension[7] =
;{
;	// header tipe data ekstensi
;        'P','H','G',
;
;        /************************************************************************************************
;        |-----------------------------------------------------------------------------------------------|
;	|	PHGD CODE (Power Height Gain Directivity)						|
;        |-----------------------------------------------------------------------------------------------|
;        |	P		|	H		|	G		|	D		|
;        |-----------------------|-----------------------|-----------------------|-----------------------|
;        |  0 rep.of  0 watts	|  0 rep.of  10 ft.	|  0 rep.of   0dBi	|  0 rep.of  omni.	|
;        |  1 rep.of  1 watts	|  1 rep.of  20 ft.	|  1 rep.of   1dBi	|  1 rep.of  NE		|
;        |  2 rep.of  4 watts	|  2 rep.of  40 ft.	|  2 rep.of   2dBi	|  2 rep.of  E		|
;        |  3 rep.of  9 watts	|  3 rep.of  80 ft.	|  3 rep.of   3dBi	|  3 rep.of  SE		|
;        |  4 rep.of  16 watts   |  4 rep.of  160 ft.	|  4 rep.of   4dBi	|  4 rep.of  S		|
;        |  5 rep.of  25 watts 	|  5 rep.of  320 ft.	|  5 rep.of   5dBi	|  5 rep.of  SW		|
;        |  6 rep.of  36 watts	|  6 rep.of  640 ft.	|  6 rep.of   6dBi	|  6 rep.of  W		|
;        |  7 rep.of  49 watts	|  7 rep.of  1280 ft.	|  7 rep.of   7dBi	|  7 rep.of  NW		|
;        |  8 rep.of  64 watts	|  8 rep.of  2560 ft.	|  8 rep.of   8dBi	|  8 rep.of  N		|
;        |  9 rep.of  81 watts	|  9 rep.of  5120 ft.	|  9 rep.of   9dBi	|-----------------------|
;        *************************************************************************************************/
;
;
;        // nilai representasi dari PHGD
;        // power : 4 watts, P = 2
;        '2',
;
;        // height above average terrain : 10 feet, H = 0
;        '0',
;
;        // antenna gain : 2dBi, G = 2
;        '2',
;
;        // antenna directivity : omnidirectional, D = 0
;        '0'
;
;};
;
;// variabel penyimpan konstanta string komentar
;flash char komentar[14] =
;{
;	// komentar
;        'L','a','b','.','S','S','T','K',' ','T','i','m','-','1'
;
;};
;
;// variabel penyimpan konstanta string status
;flash char status[48] =
;{
;	// status teks
;        '>','A','T','t','i','n','y','2','3','1','3',' ',
;        'A','P','R','S',' ','t','r','a','c','k','e','r',' ',
;        'h','a','n','d','i','k','o','g','e','s','a','n','g','@','g','m','a','i','l','.','c','o','m'
;};
;
;// variabel pengingat urutan beacon dan status
;eeprom char beacon_stat = 0;
;
;// variabel penyimpan nilai urutan interupsi, 0 ketika inisialisasi dan reset, 1 ketika TX,
;	// 2 - GAP_TIME_ ketika parsing data gps
;char xcount = 0;
;
;// variabel penyimpan tone terakhir, _1200 = 0, _2200 = 1, inisialisasi sebagai 1200Hz
;bit nada = _1200;
;
;// variabel penyimpan enablisasi bit stuffing, 0 = disable bit stuffing, 1 = enable bit stuffing
;static char bit_stuff = 0;
;
;// variabel penyimpan nilai sementara dan nilai akhir CRC-16 CCITT
;unsigned short crc;
;
;//	AKHIR DARI DEKLARASI VARIABEL GLOBAL
;
;
;/***************************************************************************************
;*
;*	KONSTANTA EVALUATOR
;*
;*/
;// cek define _1200
;#ifndef	_1200
;#error	"KONSTANTA _1200 BELUM TERDEFINISI"
;#endif
;
;// cek define _2200
;#ifndef	_2200
;#error	"KONSTANTA _2200 BELUM TERDEFINISI"
;#endif
;
;// cek define CONST_1200
;#ifndef	CONST_1200
;#error	"KONSTANTA CONST_1200 BELUM TERDEFINISI"
;#endif
;
;// cek define CONST_2200
;#ifndef	CONST_2200
;#error	"KONSTANTA CONST_2200 BELUM TERDEFINISI"
;#endif
;
;// cek define GAP_TIME_
;#ifndef	GAP_TIME_
;#error	"KONSTANTA GAP_TIME_ BELUM TERDEFINISI"
;#endif
;
;// cek define INITIAL_TIME_C
;#ifndef	INITIAL_TIME_C
;#error	"KONSTANTA INITIAL_TIME_C BELUM TERDEFINISI"
;#endif
;
;// cek define FWD_TIME_C
;#ifndef	FWD_TIME_C
;#error	"KONSTANTA FWD_TIME_C BELUM TERDEFINISI"
;#endif
;
;// cek nilai GAP_TIME_ (harus antara 15 - 30)
;#if	(GAP_TIME_ < 15)
;#error	"GAP_TIME_ harus bernilai antara 15 - 60. GAP_TIME_ yang terlalu singkat menyebabkan kepadatan traffic"
;#endif
;#if	(GAP_TIME_ > 60)
;#error	"GAP_TIME_ harus bernilai antara 15 - 60. GAP_TIME_ yang terlalu panjang menyebabkan 'loose of track'  "
;#endif
;
;// cek nilai INITIAL_TIME_C (harus kurang dari 7)
;#if	(INITIAL_TIME_C > 6)
;#error	"INITIAL_TIME_C bernilai terlalu besar"
;#endif
;#if	(INITIAL_TIME_C < 1)
;#error	"Nilai INITIAL_TIME_C tidak valid"
;#endif
;
;// cek nilai FWD_TIME_C (harus kurang dari 7)
;#if	(FWD_TIME_C > 6)
;#error	"FWD_TIME_C bernilai terlalu besar"
;#endif
;#if	(FWD_TIME_C < 1)
;#error	"Nilai FWD_TIME_C tidak valid"
;#endif
;
;//	AKHIR DARI KONSTANTA EVALUATOR
;
;
;/***************************************************************************************/
;	interrupt 		[EXT_INT1] void ext_int1_isr(void)
; 0000 01EC /***************************************************************************************
; 0000 01ED *	ABSTRAKSI	:	interupsi eksternal, ketika input TX_NOW bernilai LOW,
; 0000 01EE *				[EXT_INT1] aktif
; 0000 01EF *
; 0000 01F0 *      	INPUT		:	input TX_NOW
; 0000 01F1 *	OUTPUT		:       LED standby dan LED busy
; 0000 01F2 *	RETURN		:       tak ada
; 0000 01F3 */
; 0000 01F4 {

	.CSEG
_ext_int1_isr:
	RCALL SUBOPT_0x0
; 0000 01F5 	// matikan LED standby
; 0000 01F6         L_STBY = 0;
; 0000 01F7 
; 0000 01F8         // tunggu 250ms (bounce switch)
; 0000 01F9         delay_ms(250);
	LDI  R30,LOW(250)
	LDI  R31,HIGH(250)
	RCALL SUBOPT_0x1
; 0000 01FA 
; 0000 01FB         // kirim paket data
; 0000 01FC         kirim_paket();
	RCALL _kirim_paket
; 0000 01FD 
; 0000 01FE         // nyalakan LED standby
; 0000 01FF         L_STBY = 1;
	SBI  0x12,4
; 0000 0200 
; 0000 0201 } 	// EndOf interrupt [EXT_INT1] void ext_int1_isr(void)
	RJMP _0x81
;
;
;/***************************************************************************************/
;	interrupt 		[TIM1_OVF] void timer1_ovf_isr(void)
; 0000 0206 /***************************************************************************************
; 0000 0207 *	ABSTRAKSI  	: 	interupsi overflow TIMER 1 [TIM1_OVF], di-set overflow
; 0000 0208 *				ketika waktu telah mencapai 1 detik. Pengendali urutan
; 0000 0209 *				waktu (timeline)antara transmisi data APRS dan parsing
; 0000 020A *				data gps
; 0000 020B *
; 0000 020C *      	INPUT		:	tak ada
; 0000 020D *	OUTPUT		:       kondisi LED standby dan LED busy
; 0000 020E *	RETURN		:       tak ada
; 0000 020F */
; 0000 0210 {
_timer1_ovf_isr:
	RCALL SUBOPT_0x0
; 0000 0211 	// matikan LED stanby
; 0000 0212         L_STBY = 0;
; 0000 0213 
; 0000 0214         // tambahkan 1 nilai variabel xcount
; 0000 0215         xcount++;
	INC  R4
; 0000 0216 
; 0000 0217         // seleksi nilai variabel xcount
; 0000 0218         // kirim paket data APRS ketika timer bernilai detik ke-1
; 0000 0219         if(xcount == 1)
	LDI  R30,LOW(1)
	CP   R30,R4
	BRNE _0x14
; 0000 021A         {	// jika ya
; 0000 021B                 // kirim paket
; 0000 021C         	kirim_paket();
	RCALL _kirim_paket
; 0000 021D         }
; 0000 021E         // terima dan ekstrak data gps ketika timer detik bernilai genap saja
; 0000 021F         else if((xcount%2) == 0)
	RJMP _0x15
_0x14:
	SBRC R4,0
	RJMP _0x16
; 0000 0220         {	// jika ya
; 0000 0221         	// nyalakan LED busy
; 0000 0222                 L_BUSY = 1;
	SBI  0x12,5
; 0000 0223 
; 0000 0224                 // dapatkan data koordinat sekarang
; 0000 0225                 ekstrak_gps();
	RCALL _ekstrak_gps
; 0000 0226 
; 0000 0227                 //matikan LED busy
; 0000 0228                 L_BUSY = 0;
	CBI  0x12,5
; 0000 0229         }
; 0000 022A         // jika timer detik tidak lulus seleksi di atas, tunggu hingga GAP_TIME_ habis
; 0000 022B         else if(xcount == GAP_TIME_)
	RJMP _0x1B
_0x16:
	LDI  R30,LOW(18)
	CP   R30,R4
	BRNE _0x1C
; 0000 022C         {	// jika ya
; 0000 022D         	// reset nilai detik ke-
; 0000 022E                 xcount = 0;
	CLR  R4
; 0000 022F         }
; 0000 0230 
; 0000 0231         // nyalakan LED standby
; 0000 0232         L_STBY = 1;
_0x1C:
_0x1B:
_0x15:
	SBI  0x12,4
; 0000 0233 
; 0000 0234         // reset kembali konstanta waktu timer
; 0000 0235         timer_detik(FWD_TIME_C);
	LDI  R30,LOW(2)
	ST   -Y,R30
	RCALL _timer_detik
; 0000 0236 
; 0000 0237 }       // EndOf interrupt [TIM1_OVF] void timer1_ovf_isr(void)
_0x81:
	LD   R30,Y+
	OUT  SREG,R30
	LD   R31,Y+
	LD   R30,Y+
	LD   R27,Y+
	LD   R26,Y+
	LD   R25,Y+
	LD   R24,Y+
	LD   R23,Y+
	LD   R22,Y+
	LD   R15,Y+
	LD   R1,Y+
	LD   R0,Y+
	RETI
;
;
;/***************************************************************************************/
;	void 			kirim_paket(void)
; 0000 023C /***************************************************************************************
; 0000 023D *	ABSTRAKSI  	: 	pengendali urutan pengiriman data APRS
; 0000 023E *				penyusun protokol APRS
; 0000 023F *
; 0000 0240 *      	INPUT		:	tak ada
; 0000 0241 *	OUTPUT		:       kondisi LED dan output transistor switch TX
; 0000 0242 *	RETURN		:       tak ada
; 0000 0243 */
; 0000 0244 {
_kirim_paket:
; 0000 0245 	char i;
; 0000 0246 
; 0000 0247         // inisialisasi nilai CRC dengan 0xFFFF
; 0000 0248 	crc = 0xFFFF;
	ST   -Y,R17
;	i -> R17
	LDI  R30,LOW(65535)
	LDI  R31,HIGH(65535)
	MOVW R6,R30
; 0000 0249 
; 0000 024A         // tambahkan counter pancar
; 0000 024B         beacon_stat++;
	RCALL SUBOPT_0x2
	SUBI R30,-LOW(1)
	RCALL __EEPROMWRB
	SUBI R30,LOW(1)
; 0000 024C 
; 0000 024D         // nyalakan LED TX dan PTT switch
; 0000 024E 	PTT = 1;
	SBI  0x18,3
; 0000 024F 
; 0000 0250         // tunggu 500ms
; 0000 0251         delay_ms(500);
	RCALL SUBOPT_0x3
; 0000 0252 
; 0000 0253         /**********************************************************************************
; 0000 0254 
; 0000 0255         			APRS AX.25 PROTOCOL
; 0000 0256 
; 0000 0257         |------------------------------------------------------------------------
; 0000 0258         |   opn. FLAG	|	DESTINATION	|	SOURCE	|	DIGI'S	| CONTROL...
; 0000 0259         |---------------|-----------------------|---------------|---------------|
; 0000 025A         |   0x7E 1Bytes |	7 Bytes		|       7 Bytes |  0 - 56 Bytes	|
; 0000 025B         |------------------------------------------------------------------------
; 0000 025C 
; 0000 025D         	-----------------------------------------------------------------
; 0000 025E         DIGI'S..|	CONTROL FIELD	|	PROTOCOL ID	|	INFO	| FCS...
; 0000 025F                 |-----------------------|-----------------------|---------------|
; 0000 0260                 |    0x03 1 Bytes	|     0xF0 1 Bytes	|  0 - 256 Bytes|
; 0000 0261                 -----------------------------------------------------------------
; 0000 0262 
; 0000 0263         	--------------------------------|
; 0000 0264         INFO... |	FCS	|   cls. FLAG	|
; 0000 0265                 |---------------|---------------|
; 0000 0266                 |	2 Bytes	|   0x7E 1Bytes |
; 0000 0267                 --------------------------------|
; 0000 0268 
; 0000 0269         ************************************************************************************/
; 0000 026A 
; 0000 026B         // kirim karakter opening flag
; 0000 026C         for(i=0;i<TX_DELAY_;i++)
	LDI  R17,LOW(0)
_0x22:
	CPI  R17,40
	BRSH _0x23
; 0000 026D         	kirim_karakter(FLAG_);
	LDI  R30,LOW(126)
	RCALL SUBOPT_0x4
	SUBI R17,-1
	RJMP _0x22
_0x23:
; 0000 0270 bit_stuff = 0;
	RCALL SUBOPT_0x5
; 0000 0271 
; 0000 0272         // kirimkan field : destination, source, PATH 1, PATH 2, control, Protocol ID, dan
; 0000 0273         	// data type ID
; 0000 0274         for(i=0;i<28;i++)
	LDI  R17,LOW(0)
_0x25:
	CPI  R17,28
	BRSH _0x26
; 0000 0275         	kirim_karakter(data_1[i]);
	RCALL SUBOPT_0x6
	SUBI R30,LOW(-_data_1*2)
	SBCI R31,HIGH(-_data_1*2)
	RCALL SUBOPT_0x7
	SUBI R17,-1
	RJMP _0x25
_0x26:
; 0000 0278 kirim_karakter(0x03);
	LDI  R30,LOW(3)
	RCALL SUBOPT_0x4
; 0000 0279 
; 0000 027A         // krimkan protocol ID
; 0000 027B         kirim_karakter(PROTOCOL_ID_);
	LDI  R30,LOW(240)
	RCALL SUBOPT_0x4
; 0000 027C 
; 0000 027D         // jika sudah 10 kali memancar,
; 0000 027E         if(beacon_stat == 10)
	RCALL SUBOPT_0x2
	CPI  R30,LOW(0xA)
	BRNE _0x27
; 0000 027F         {
; 0000 0280         	// jika ya
; 0000 0281                 // kirim teks status
; 0000 0282                 for(i=0;i<48;i++)
	LDI  R17,LOW(0)
_0x29:
	CPI  R17,48
	BRSH _0x2A
; 0000 0283                 	kirim_karakter(status[i]);
	RCALL SUBOPT_0x6
	SUBI R30,LOW(-_status*2)
	SBCI R31,HIGH(-_status*2)
	RCALL SUBOPT_0x7
	SUBI R17,-1
	RJMP _0x29
_0x2A:
; 0000 0286 beacon_stat = 0;
	LDI  R26,LOW(_beacon_stat)
	LDI  R27,HIGH(_beacon_stat)
	LDI  R30,LOW(0)
	RCALL __EEPROMWRB
; 0000 0287 
; 0000 0288                 // lompat ke kirim crc
; 0000 0289                 goto lompat;
	RJMP _0x2B
; 0000 028A         }
; 0000 028B 
; 0000 028C         // krimkan tipe data posisi
; 0000 028D         kirim_karakter(TD_POSISI_);
_0x27:
	LDI  R30,LOW(61)
	RCALL SUBOPT_0x4
; 0000 028E 
; 0000 028F         // kirimkan posisi lintang
; 0000 0290         for(i=0;i<8;i++)
	LDI  R17,LOW(0)
_0x2D:
	CPI  R17,8
	BRSH _0x2E
; 0000 0291         	kirim_karakter(posisi_lat[i]);
	LDI  R26,LOW(_posisi_lat)
	ADD  R26,R17
	LD   R30,X
	RCALL SUBOPT_0x4
	SUBI R17,-1
	RJMP _0x2D
_0x2E:
; 0000 0294 kirim_karakter('/');
	LDI  R30,LOW(47)
	RCALL SUBOPT_0x4
; 0000 0295 
; 0000 0296         // kirimkan posisi bujur
; 0000 0297 	for(i=0;i<9;i++)
	LDI  R17,LOW(0)
_0x30:
	CPI  R17,9
	BRSH _0x31
; 0000 0298         	kirim_karakter(posisi_long[i]);
	LDI  R26,LOW(_posisi_long)
	ADD  R26,R17
	LD   R30,X
	RCALL SUBOPT_0x4
	SUBI R17,-1
	RJMP _0x30
_0x31:
; 0000 029B kirim_karakter('7');
	LDI  R30,LOW(55)
	RCALL SUBOPT_0x4
; 0000 029C 
; 0000 029D         // hanya kirim PHGD code dan komentar pada pancaran ke-5 saja
; 0000 029E         if(beacon_stat == 5)
	RCALL SUBOPT_0x2
	CPI  R30,LOW(0x5)
	BRNE _0x32
; 0000 029F         {
; 0000 02A0         	// kirimkan field informasi : data ekstensi tipe PHGD
; 0000 02A1         	for(i=0;i<7;i++)
	LDI  R17,LOW(0)
_0x34:
	CPI  R17,7
	BRSH _0x35
; 0000 02A2         		kirim_karakter(data_extension[i]);
	RCALL SUBOPT_0x6
	SUBI R30,LOW(-_data_extension*2)
	SBCI R31,HIGH(-_data_extension*2)
	RCALL SUBOPT_0x7
	SUBI R17,-1
	RJMP _0x34
_0x35:
; 0000 02A5 for(i=0;i<14;i++)
	LDI  R17,LOW(0)
_0x37:
	CPI  R17,14
	BRSH _0x38
; 0000 02A6         		kirim_karakter(komentar[i]);
	RCALL SUBOPT_0x6
	SUBI R30,LOW(-_komentar*2)
	SBCI R31,HIGH(-_komentar*2)
	RCALL SUBOPT_0x7
	SUBI R17,-1
	RJMP _0x37
_0x38:
; 0000 02A7 }
; 0000 02A8 
; 0000 02A9 
; 0000 02AA         // label lompatan
; 0000 02AB         lompat:
_0x32:
_0x2B:
; 0000 02AC 
; 0000 02AD         // kirimkan field : FCS (CRC-16 CCITT)
; 0000 02AE         kirim_crc();
	RCALL _kirim_crc
; 0000 02AF 
; 0000 02B0         // kirimkan karakter closing flag
; 0000 02B1         for(i=0;i<TX_TAIL_;i++)
	LDI  R17,LOW(0)
_0x3A:
	CPI  R17,2
	BRSH _0x3B
; 0000 02B2         	kirim_karakter(FLAG_);
	LDI  R30,LOW(126)
	RCALL SUBOPT_0x4
	SUBI R17,-1
	RJMP _0x3A
_0x3B:
; 0000 02B5 PORTB.3  = 0;
	CBI  0x18,3
; 0000 02B6 
; 0000 02B7 
; 0000 02B8 }       // EndOf void kirim_paket(void)
_0x2060003:
	LD   R17,Y+
	RET
;
;
;/***************************************************************************************/
;	void 			kirim_crc(void)
; 0000 02BD /***************************************************************************************
; 0000 02BE *	ABSTRAKSI  	: 	Pengendali urutan pengiriman data CRC-16 CCITT Penyusun
; 0000 02BF *				nilai 8 MSB dan 8 LSB dari nilai CRC yang telah dihitung.
; 0000 02C0 *				Generator polinomial, G(x) = x^16 + x^12 + x^5 + 1 tanpa
; 0000 02C1 *				leading one.
; 0000 02C2 *
; 0000 02C3 *      	INPUT		:	tak ada
; 0000 02C4 *	OUTPUT		:       tak ada
; 0000 02C5 *	RETURN		:       tak ada
; 0000 02C6 */
; 0000 02C7 {
_kirim_crc:
; 0000 02C8 	static unsigned char crc_lo;
; 0000 02C9 	static unsigned char crc_hi;
; 0000 02CA 
; 0000 02CB         // Exor-kan nilai CRC terakhir dengan 0xFF, masukkan ke 8 LSB
; 0000 02CC         crc_lo = crc ^ 0xFF;
	LDI  R30,LOW(255)
	EOR  R30,R6
	STS  _crc_lo_S0000005000,R30
; 0000 02CD 
; 0000 02CE         // geser kanan 8 bit dan Exor-kan nilai CRC terakhir dengan 0xFF, masukkan ke 8 MSB
; 0000 02CF         crc_hi = (crc >> 8) ^ 0xFF;
	MOV  R30,R7
	ANDI R31,HIGH(0x0)
	LDI  R26,LOW(255)
	EOR  R30,R26
	STS  _crc_hi_S0000005000,R30
; 0000 02D0 
; 0000 02D1         // kirim 8 LSB
; 0000 02D2         kirim_karakter(crc_lo);
	LDS  R30,_crc_lo_S0000005000
	RCALL SUBOPT_0x4
; 0000 02D3 
; 0000 02D4         // kirim 8 MSB
; 0000 02D5         kirim_karakter(crc_hi);
	LDS  R30,_crc_hi_S0000005000
	RCALL SUBOPT_0x4
; 0000 02D6 
; 0000 02D7 }       // EndOf void kirim_crc(void)
	RET
;
;
;/***************************************************************************************/
;	void 			kirim_karakter(unsigned char input)
; 0000 02DC /***************************************************************************************
; 0000 02DD *	ABSTRAKSI  	: 	mengirim data APRS karakter-demi-karakter, menghitung FCS
; 0000 02DE *				field dan melakukan bit stuffing. Polarisasi data adalah
; 0000 02DF *				NRZI (Non Return to Zero, Inverted). Bit dikirimkan sebagai
; 0000 02E0 *				bit terakhir yang ditahan jika bit masukan adalah bit 1.
; 0000 02E1 *				Bit dikirimkan sebagai bit terakhir yang di-invert jika bit
; 0000 02E2 *				masukan adalah bit 0. Tone 1200Hz dan 2200Hz masing - masing
; 0000 02E3 * 				merepresentasikan bit 0 dan 1 atau sebaliknya. Polarisasi
; 0000 02E4 *				tone adalah tidak penting dalam polarisasi data NRZI.
; 0000 02E5 *
; 0000 02E6 *      	INPUT		:	byte data protokol APRS
; 0000 02E7 *	OUTPUT		:       tak ada
; 0000 02E8 *	RETURN		:       tak ada
; 0000 02E9 */
; 0000 02EA {
_kirim_karakter:
	PUSH R15
; 0000 02EB 	char i;
; 0000 02EC 	bit in_bit;
; 0000 02ED 
; 0000 02EE         // kirimkan setiap byte data (8 bit)
; 0000 02EF 	for(i=0;i<8;i++)
	ST   -Y,R17
;	input -> Y+1
;	i -> R17
;	in_bit -> R15.0
	LDI  R17,LOW(0)
_0x3F:
	CPI  R17,8
	BRSH _0x40
; 0000 02F0         {
; 0000 02F1         	// ambil 1 bit berurutan dari LSB ke MSB setiap perulangan for 0 - 7
; 0000 02F2                 in_bit = (input >> i) & 0x01;
	MOV  R30,R17
	LDD  R26,Y+1
	RCALL __LSRB12
	BST  R30,0
	BLD  R15,0
; 0000 02F3 
; 0000 02F4                 // jika data adalah flag, nol-kan pengingat bit stuffing
; 0000 02F5                 if(input==0x7E)	{bit_stuff = 0;}
	CPI  R26,LOW(0x7E)
	BRNE _0x41
	RCALL SUBOPT_0x5
; 0000 02F6 
; 0000 02F7                 // jika bukan flag, hitung nilai CRC dari bit data saat ini
; 0000 02F8                 else		{hitung_crc(in_bit);}
	RJMP _0x42
_0x41:
	LDI  R30,0
	SBRC R15,0
	LDI  R30,1
	ST   -Y,R30
	RCALL _hitung_crc
_0x42:
; 0000 02F9 
; 0000 02FA                 // jika bit data saat ini adalah
; 0000 02FB                 // nol
; 0000 02FC                 if(!in_bit)
	SBRS R15,0
; 0000 02FD                 {	// jika ya
; 0000 02FE                 	// ubah tone dan bentuk gelombang sinus
; 0000 02FF                         ubah_nada();
	RJMP _0x7F
; 0000 0300 
; 0000 0301                         // nol-kan pengingat bit stuffing
; 0000 0302                         bit_stuff = 0;
; 0000 0303                 }
; 0000 0304                 // satu
; 0000 0305                 else
; 0000 0306                 {	// jika ya
; 0000 0307                 	// jaga tone dan bentuk gelombang sinus
; 0000 0308                         set_nada(nada);
	LDI  R30,0
	SBIC 0x13,1
	LDI  R30,1
	ST   -Y,R30
	RCALL _set_nada
; 0000 0309 
; 0000 030A                         // hitung sebagai bit stuffing (bit satu berurut) tambahkan 1 nilai bit stuffing
; 0000 030B                         bit_stuff++;
	LDS  R30,_bit_stuff_G000
	SUBI R30,-LOW(1)
	STS  _bit_stuff_G000,R30
; 0000 030C 
; 0000 030D                         // jika sudah terjadi bit satu berurut sebanyak 5 kali
; 0000 030E                         if(bit_stuff==5)
	LDS  R26,_bit_stuff_G000
	CPI  R26,LOW(0x5)
	BRNE _0x45
; 0000 030F                         {
; 0000 0310                         	// ubah tone dan bentuk gelombang sinus
; 0000 0311                                 ubah_nada();
_0x7F:
	RCALL _ubah_nada
; 0000 0312 
; 0000 0313                                 // nol-kan pengingat bit stuffing
; 0000 0314                                 bit_stuff = 0;
	RCALL SUBOPT_0x5
; 0000 0315 
; 0000 0316                         }
; 0000 0317                 }
_0x45:
; 0000 0318         }
	SUBI R17,-1
	RJMP _0x3F
_0x40:
; 0000 0319 
; 0000 031A }      // EndOf void kirim_karakter(unsigned char input)
	LDD  R17,Y+0
	ADIW R28,2
	POP  R15
	RET
;
;
;/***************************************************************************************/
;	void 			hitung_crc(char in_crc)
; 0000 031F /***************************************************************************************
; 0000 0320 *	ABSTRAKSI  	: 	menghitung nilai CRC-16 CCITT dari tiap bit data yang terkirim,
; 0000 0321 *				generator polinomial, G(x) = x^16 + x^12 + x^5 + 1 tanpa
; 0000 0322 *				leading one
; 0000 0323 *
; 0000 0324 *      	INPUT		:	bit data yang terkirim
; 0000 0325 *	OUTPUT		:       tak ada
; 0000 0326 *	RETURN		:       tak ada
; 0000 0327 */
; 0000 0328 {
_hitung_crc:
; 0000 0329 	static unsigned short xor_in;
; 0000 032A 
; 0000 032B         // simpan nilai Exor dari CRC sementara dengan bit data yang baru terkirim
; 0000 032C 	xor_in = crc ^ in_crc;
;	in_crc -> Y+0
	LD   R30,Y
	LDI  R31,0
	EOR  R30,R6
	EOR  R31,R7
	STS  _xor_in_S0000007000,R30
	STS  _xor_in_S0000007000+1,R31
; 0000 032D 
; 0000 032E         // geser kanan nilai CRC sebanyak 1 bit
; 0000 032F 	crc >>= 1;
	LSR  R7
	ROR  R6
; 0000 0330 
; 0000 0331         // jika hasil nilai Exor di-and-kan dengan satu bernilai satu
; 0000 0332         if(xor_in & 0x01)
	LDS  R30,_xor_in_S0000007000
	ANDI R30,LOW(0x1)
	BREQ _0x46
; 0000 0333         	// maka nilai CRC di-Exor-kan dengan generator polinomial
; 0000 0334                 crc ^= 0x8408;
	LDI  R30,LOW(33800)
	LDI  R31,HIGH(33800)
	__EORWRR 6,7,30,31
; 0000 0335 
; 0000 0336 }      // EndOf void hitung_crc(char in_crc)
_0x46:
	RJMP _0x2060002
;
;
;/***************************************************************************************/
;	void 			ubah_nada(void)
; 0000 033B /***************************************************************************************
; 0000 033C *	ABSTRAKSI  	: 	Menukar seting tone terakhir dengan tone yang baru. Tone
; 0000 033D *				1200Hz dan 2200Hz masing - masing merepresentasikan bit
; 0000 033E *				0 dan 1 atau sebaliknya. Polarisasi tone adalah tidak
; 0000 033F *				penting dalam polarisasi data NRZI.
; 0000 0340 *
; 0000 0341 *      	INPUT		:	tak ada
; 0000 0342 *	OUTPUT		:       tak ada
; 0000 0343 *	RETURN		:       tak ada
; 0000 0344 */
; 0000 0345 {
_ubah_nada:
; 0000 0346 	// jika tone terakhir adalah :
; 0000 0347         // 1200Hz
; 0000 0348         if(nada ==_1200)
	SBIC 0x13,1
	RJMP _0x47
; 0000 0349 	{	// jika ya
; 0000 034A         	// ubah tone saat ini menjadi 2200Hz
; 0000 034B                 nada = _2200;
	SBI  0x13,1
; 0000 034C 
; 0000 034D                 // bangkitkan gelombang sinus 2200Hz
; 0000 034E         	set_nada(nada);
	RJMP _0x80
; 0000 034F 	}
; 0000 0350         // 2200Hz
; 0000 0351         else
_0x47:
; 0000 0352         {	// jika ya
; 0000 0353         	// ubah tone saat ini menjadi 1200Hz
; 0000 0354                 nada = _1200;
	CBI  0x13,1
; 0000 0355 
; 0000 0356                 // bangkitkan gelombang sinus 1200Hz
; 0000 0357         	set_nada(nada);
_0x80:
	LDI  R30,0
	SBIC 0x13,1
	LDI  R30,1
	ST   -Y,R30
	RCALL _set_nada
; 0000 0358         }
; 0000 0359 
; 0000 035A }       // EndOf void ubah_nada(void)
	RET
;
;
;/***************************************************************************************/
;	void 			set_dac(char value)
; 0000 035F /***************************************************************************************
; 0000 0360 *	ABSTRAKSI  	: 	Men-set dan reset output DAC sebagai bilangan biner yang
; 0000 0361 *				merepresentasikan nilai diskrit dari gelombang sinus yang
; 0000 0362 *				sedang dibentuk saat ini sehingga membentuk tegangan sampling
; 0000 0363 *				dari gelombang.
; 0000 0364 *
; 0000 0365 *      	INPUT		:	nilai matrix rekonstruksi diskrit gelombang sinusoid
; 0000 0366 *	OUTPUT		:       DAC 0 - 3, tegangan kontinyu pada output Low Pass Filter
; 0000 0367 *	RETURN		:       tak ada
; 0000 0368 */
; 0000 0369 {
_set_dac:
; 0000 036A 	// ambil nilai LSB dari matrix rekonstruksi dan set sebagai DAC-0
; 0000 036B         DAC_0 = value & 0x01;
;	value -> Y+0
	LD   R30,Y
	ANDI R30,LOW(0x1)
	BRNE _0x4D
	CBI  0x18,7
	RJMP _0x4E
_0x4D:
	SBI  0x18,7
_0x4E:
; 0000 036C 
; 0000 036D         // ambil nilai dari matrix rekonstruksi, geser kanan 1 bit, ambil bit paling kanan
; 0000 036E         	// dan set sebagai DAC-1
; 0000 036F         DAC_1 =( value >> 1 ) & 0x01;
	LD   R30,Y
	LSR  R30
	ANDI R30,LOW(0x1)
	BRNE _0x4F
	CBI  0x18,6
	RJMP _0x50
_0x4F:
	SBI  0x18,6
_0x50:
; 0000 0370 
; 0000 0371         // ambil nilai dari matrix rekonstruksi, geser kanan 2 bit, ambil bit paling kanan
; 0000 0372         	// dan set sebagai DAC-2
; 0000 0373         DAC_2 =( value >> 2 ) & 0x01;
	LD   R30,Y
	LSR  R30
	LSR  R30
	ANDI R30,LOW(0x1)
	BRNE _0x51
	CBI  0x18,5
	RJMP _0x52
_0x51:
	SBI  0x18,5
_0x52:
; 0000 0374 
; 0000 0375         // ambil nilai dari matrix rekonstruksi, geser kanan 3 bit, ambil bit tersebut dan
; 0000 0376         	// set sebagai DAC-3 (MSB)
; 0000 0377         DAC_3 =( value >> 3 ) & 0x01;
	LD   R30,Y
	LSR  R30
	LSR  R30
	LSR  R30
	ANDI R30,LOW(0x1)
	BRNE _0x53
	CBI  0x18,4
	RJMP _0x54
_0x53:
	SBI  0x18,4
_0x54:
; 0000 0378 
; 0000 0379 }      	// EndOf void set_dac(char value)
_0x2060002:
	ADIW R28,1
	RET
;
;
;/***************************************************************************************/
;	void 			set_nada(char i_nada)
; 0000 037E /***************************************************************************************
; 0000 037F *	ABSTRAKSI  	: 	Membentuk baudrate serta frekensi tone 1200Hz dan 2200Hz
; 0000 0380 *				dari konstanta waktu. Men-setting nilai DAC. Lakukan fine
; 0000 0381 *				tuning pada jumlah masing - masing perulangan for dan
; 0000 0382 *				konstanta waktu untuk meng-adjust parameter baudrate dan
; 0000 0383 *				frekuensi tone.
; 0000 0384 *
; 0000 0385 *      	INPUT		:	nilai frekuensi tone yang akan ditransmisikan
; 0000 0386 *	OUTPUT		:       nilai DAC
; 0000 0387 *	RETURN		:       tak ada
; 0000 0388 */
; 0000 0389 {
_set_nada:
; 0000 038A 	char i;
; 0000 038B 
; 0000 038C         // jika frekuensi tone yang akan segera dipancarkan adalah :
; 0000 038D         // 1200Hz
; 0000 038E 	if(i_nada == _1200)
	ST   -Y,R17
;	i_nada -> Y+1
;	i -> R17
	LDD  R30,Y+1
	CPI  R30,0
	BRNE _0x55
; 0000 038F     	{
; 0000 0390         	// jika ya
; 0000 0391         	for(i=0; i<16; i++)
	LDI  R17,LOW(0)
_0x57:
	CPI  R17,16
	BRSH _0x58
; 0000 0392         	{
; 0000 0393                 	// set nilai DAC sesuai urutan matrix rekonstruksi gelombang sinus
; 0000 0394                 		// dan urutan perulangan for 0 - 15
; 0000 0395                 	set_dac(matrix[i]);
	RCALL SUBOPT_0x6
	RCALL SUBOPT_0x8
; 0000 0396 
; 0000 0397                         // bangkitkan frekuensi 1200Hz dari konstanta waktu
; 0000 0398         		delay_us(CONST_1200);
	__DELAY_USB 170
; 0000 0399         	}
	SUBI R17,-1
	RJMP _0x57
_0x58:
; 0000 039A     	}
; 0000 039B         // 2200Hz
; 0000 039C     	else
	RJMP _0x59
_0x55:
; 0000 039D     	{
; 0000 039E         	// jika ya
; 0000 039F         	for(i=0; i<16; i++)
	LDI  R17,LOW(0)
_0x5B:
	CPI  R17,16
	BRSH _0x5C
; 0000 03A0         	{
; 0000 03A1                 	// set nilai DAC sesuai urutan matrix rekonstruksi gelombang sinus
; 0000 03A2                 		// dan urutan perulangan for 0 - 15
; 0000 03A3                 	set_dac(matrix[i]);
	RCALL SUBOPT_0x6
	RCALL SUBOPT_0x8
; 0000 03A4 
; 0000 03A5                         // bangkitkan frekuensi 2200Hz dari konstanta waktu
; 0000 03A6                 	delay_us(CONST_2200);
	__DELAY_USB 81
; 0000 03A7                 }
	SUBI R17,-1
	RJMP _0x5B
_0x5C:
; 0000 03A8                 // sekali lagi, untuk mengatur baudrate lakukan fine tune pada jumlah for
; 0000 03A9                 for(i=0; i<13; i++)
	LDI  R17,LOW(0)
_0x5E:
	CPI  R17,13
	BRSH _0x5F
; 0000 03AA                 {
; 0000 03AB                 	// set nilai DAC sesuai urutan matrix rekonstruksi gelombang sinus
; 0000 03AC                 		// dan urutan perulangan for
; 0000 03AD                 	set_dac(matrix[i]);
	RCALL SUBOPT_0x6
	RCALL SUBOPT_0x8
; 0000 03AE 
; 0000 03AF                         // bangkitkan frekuensi 2200Hz dari konstanta waktu
; 0000 03B0                 	delay_us(CONST_2200);
	__DELAY_USB 81
; 0000 03B1                 }
	SUBI R17,-1
	RJMP _0x5E
_0x5F:
; 0000 03B2     	}
_0x59:
; 0000 03B3 
; 0000 03B4 } 	// EndOf void set_nada(char i_nada)
	LDD  R17,Y+0
	ADIW R28,2
	RET
;
;
;/***************************************************************************************/
;	void 			getComma(void)
; 0000 03B9 /***************************************************************************************
; 0000 03BA *	ABSTRAKSI  	: 	Menunggu data RX serial berupa karakter koma dan segera
; 0000 03BB *				kembali pada fungsi yang memanggilnya.
; 0000 03BC *
; 0000 03BD *      	INPUT		:	RX data serial $GPGLL gps
; 0000 03BE *	OUTPUT		:       tak ada
; 0000 03BF *	RETURN		:       tak ada
; 0000 03C0 */
; 0000 03C1 {
_getComma:
; 0000 03C2 	// jika data yang diterima bukan karakter koma, terima terus
; 0000 03C3         	// jika data yang diterima adalah koma, keluar
; 0000 03C4         while(getchar() != ',');
_0x60:
	RCALL _getchar
	CPI  R30,LOW(0x2C)
	BRNE _0x60
; 0000 03C5 
; 0000 03C6 }      	// EndOf void getComma(void)
	RET
;
;
;/***************************************************************************************/
;	void 			ekstrak_gps(void)
; 0000 03CB /***************************************************************************************
; 0000 03CC *	ABSTRAKSI  	: 	Menunggu interupsi RX data serial dari USART, memparsing
; 0000 03CD *				data $GPGLL yang diterima menjadi data posisi, dan mengupdate
; 0000 03CE *				data variabel posisi.
; 0000 03CF *
; 0000 03D0 *      	INPUT		:	RX data serial $GPGLL gps
; 0000 03D1 *	OUTPUT		:       tak ada
; 0000 03D2 *	RETURN		:       tak ada
; 0000 03D3 */
; 0000 03D4 {
_ekstrak_gps:
; 0000 03D5 	int i;
; 0000 03D6         static char buff_posisi[17];
; 0000 03D7 
; 0000 03D8         // aktifkan USART param. : 4800baudrate, 8, N, 1
; 0000 03D9         init_usart();
	RCALL __SAVELOCR2
;	i -> R16,R17
	RCALL _init_usart
; 0000 03DA 
; 0000 03DB         /************************************************************************************************
; 0000 03DC         	$GPGLL - GLL - Geographic Position Latitude / Longitude
; 0000 03DD 
; 0000 03DE                 Contoh : $GPGLL,3723.2475,N,12158.3416,W,161229.487,A*2C
; 0000 03DF 
; 0000 03E0         |-----------------------------------------------------------------------------------------------|
; 0000 03E1         |	Nama		| 	Contoh		|		Deskripsi			|
; 0000 03E2         |-----------------------|-----------------------|-----------------------------------------------|
; 0000 03E3         |	Message ID	|	$GPGLL		|	header protokol GLL			|
; 0000 03E4         |	Latitude	|	3723.2475	|	ddmm.mmmm 	, d=degree, m=minute	|
; 0000 03E5         |	N/S indicator	|	N		|	N=utara, S=selatan			|
; 0000 03E6         |	Longitude	|	12158.3416	|	dddmm.mmmm	, d=degree, m=minute	|
; 0000 03E7         |	W/E indicator	|	W		|	W=barat, E=timur			|
; 0000 03E8         |	Waktu UTC (GMT)	|	161229.487	|	HHMMSS.SS  ,H=hour, M=minute, S=second	|
; 0000 03E9         |	Status		|	A		|	A=data valid, V=data invalid		|
; 0000 03EA         |	Checksum	|	*2C		|						|
; 0000 03EB         |-----------------------------------------------------------------------------------------------|
; 0000 03EC 
; 0000 03ED         	Sumber : GPS SiRF EM-406A datasheet
; 0000 03EE 
; 0000 03EF         *************************************************************************************************/
; 0000 03F0 
; 0000 03F1         // jika data yang diterima bukan karakter $, terima terus
; 0000 03F2         	// jika data yang diterima adalah $, lanjutkan
; 0000 03F3         while(getchar() != '$');
_0x63:
	RCALL _getchar
	CPI  R30,LOW(0x24)
	BRNE _0x63
; 0000 03F4 
; 0000 03F5         // tunggu dan terima data tanpa proses lebih lanjut (lewatkan karakter G)
; 0000 03F6 	getchar();
	RCALL _getchar
; 0000 03F7 
; 0000 03F8         // tunggu dan terima data tanpa proses lebih lanjut (lewatkan karakter P)
; 0000 03F9         getchar();
	RCALL _getchar
; 0000 03FA 
; 0000 03FB         // tunggu data, jika yang diterima adalah karakter G
; 0000 03FC         if(getchar() == 'G')
	RCALL _getchar
	CPI  R30,LOW(0x47)
	BREQ PC+2
	RJMP _0x66
; 0000 03FD         {
; 0000 03FE         	// maka
; 0000 03FF         	// tunggu data, jika yang diterima adalah karakter L
; 0000 0400                 if(getchar() == 'L')
	RCALL _getchar
	CPI  R30,LOW(0x4C)
	BREQ PC+2
	RJMP _0x67
; 0000 0401         	{
; 0000 0402                 	// maka
; 0000 0403                         // tunggu data, jika yang diterima adalah karakter L
; 0000 0404                         if(getchar() == 'L')
	RCALL _getchar
	CPI  R30,LOW(0x4C)
	BREQ PC+2
	RJMP _0x68
; 0000 0405                 	{
; 0000 0406                         	// maka
; 0000 0407                                 // tunggu koma dan lanjutkan
; 0000 0408                                 getComma();
	RCALL _getComma
; 0000 0409 
; 0000 040A                                 // ambil 8 byte data berurut dan masukkan dalam buffer data
; 0000 040B                         	for(i=0; i<7; i++)	buff_posisi[i] = getchar();
	RCALL SUBOPT_0x9
_0x6A:
	__CPWRN 16,17,7
	BRGE _0x6B
	MOV  R30,R16
	SUBI R30,-LOW(_buff_posisi_S000000C000)
	PUSH R30
	RCALL _getchar
	POP  R26
	ST   X,R30
	RCALL SUBOPT_0xA
	RJMP _0x6A
_0x6B:
; 0000 040E getchar();
	RCALL SUBOPT_0xB
; 0000 040F 
; 0000 0410                                 // tunggu dan terima data tanpa proses lebih lanjut
; 0000 0411                                 getchar();
; 0000 0412 
; 0000 0413                                 // tunggu koma dan lanjutkan
; 0000 0414                                 getComma();
; 0000 0415 
; 0000 0416                                 // ambil 1 byte data dan masukkan dalam buffer data
; 0000 0417                                 buff_posisi[7] = getchar();
	__PUTB1MN _buff_posisi_S000000C000,7
; 0000 0418 
; 0000 0419                                 // tunggu koma dan lanjutkan
; 0000 041A                                 getComma();
	RCALL _getComma
; 0000 041B 
; 0000 041C                                 // ambil 9 byte data berurut dan masukkan dalam buffer data
; 0000 041D                                 for(i=0; i<8; i++)	buff_posisi[i+8] = getchar();
	RCALL SUBOPT_0x9
_0x6D:
	__CPWRN 16,17,8
	BRGE _0x6E
	MOV  R30,R16
	__ADDB1MN _buff_posisi_S000000C000,8
	PUSH R30
	RCALL _getchar
	POP  R26
	ST   X,R30
	RCALL SUBOPT_0xA
	RJMP _0x6D
_0x6E:
; 0000 0420 getchar();
	RCALL SUBOPT_0xB
; 0000 0421 
; 0000 0422                                 // tunggu dan terima data tanpa proses lebih lanjut
; 0000 0423                                 getchar();
; 0000 0424 
; 0000 0425                                 // tunggu koma dan lanjutkan
; 0000 0426                                 getComma();
; 0000 0427 
; 0000 0428                                 // ambil 1 byte data dan masukkan dalam buffer data
; 0000 0429                                 buff_posisi[16] = getchar();
	__PUTB1MN _buff_posisi_S000000C000,16
; 0000 042A 
; 0000 042B                                 // segera matikan USART untuk menghindari interupsi [USART_RXC]
; 0000 042C                                 clear_usart();
	RCALL _clear_usart
; 0000 042D 
; 0000 042E                                 // pindahkan data dari buffer kedalam variabel posisi
; 0000 042F                                 for(i=0;i<8;i++)	{posisi_lat[i]=buff_posisi[i];}
	RCALL SUBOPT_0x9
_0x70:
	__CPWRN 16,17,8
	BRGE _0x71
	MOV  R30,R16
	SUBI R30,-LOW(_posisi_lat)
	MOV  R0,R30
	LDI  R26,LOW(_buff_posisi_S000000C000)
	ADD  R26,R16
	LD   R30,X
	MOV  R26,R0
	ST   X,R30
	RCALL SUBOPT_0xA
	RJMP _0x70
_0x71:
; 0000 0430         			for(i=0;i<9;i++)	{posisi_long[i+9]=buff_posisi[i+8];}
	RCALL SUBOPT_0x9
_0x73:
	__CPWRN 16,17,9
	BRGE _0x74
	MOV  R30,R16
	__ADDB1MN _posisi_long,9
	MOV  R26,R30
	MOV  R30,R16
	__ADDB1MN _buff_posisi_S000000C000,8
	LD   R30,Z
	ST   X,R30
	RCALL SUBOPT_0xA
	RJMP _0x73
_0x74:
; 0000 0431 
; 0000 0432                         }
; 0000 0433                 }
_0x68:
; 0000 0434         }
_0x67:
; 0000 0435 
; 0000 0436 } 	// EndOf void ekstrak_gps(void)
_0x66:
	RCALL __LOADLOCR2P
	RET
;
;
;/***************************************************************************************/
;	void 			init_usart(void)
; 0000 043B /***************************************************************************************
; 0000 043C *	ABSTRAKSI  	: 	Setting parameter USART : RX only, 4800baud, 8, N, 1
; 0000 043D *
; 0000 043E *      	INPUT		:	tak ada
; 0000 043F *	OUTPUT		:       tak ada
; 0000 0440 *	RETURN		:       tak ada
; 0000 0441 */
; 0000 0442 {
_init_usart:
; 0000 0443 	// set parameter 4800baud, 8, N, 1
; 0000 0444 	UCSRA=0x00;
	LDI  R30,LOW(0)
	OUT  0xB,R30
; 0000 0445 	UCSRB=0x90;
	LDI  R30,LOW(144)
	OUT  0xA,R30
; 0000 0446 	UCSRC=0x06;
	LDI  R30,LOW(6)
	OUT  0x3,R30
; 0000 0447 	UBRRH=0x00;
	LDI  R30,LOW(0)
	OUT  0x2,R30
; 0000 0448 	UBRRL=0x8F;
	LDI  R30,LOW(143)
	RJMP _0x2060001
; 0000 0449 
; 0000 044A }       // EndOf void init_usart(void)
;
;
;/***************************************************************************************/
;	void 			clear_usart(void)
; 0000 044F /***************************************************************************************
; 0000 0450 *	ABSTRAKSI  	: 	Me-nonaktifkan dan menghapus parameter USART
; 0000 0451 *
; 0000 0452 *      	INPUT		:	tak ada
; 0000 0453 *	OUTPUT		:       tak ada
; 0000 0454 *	RETURN		:       tak ada
; 0000 0455 */
; 0000 0456 {
_clear_usart:
; 0000 0457 	// hapus parameter terakhir dari USART
; 0000 0458         UCSRA=0;
	LDI  R30,LOW(0)
	OUT  0xB,R30
; 0000 0459 	UCSRB=0;
	OUT  0xA,R30
; 0000 045A 	UCSRC=0;
	OUT  0x3,R30
; 0000 045B 	UBRRH=0;
	OUT  0x2,R30
; 0000 045C 	UBRRL=0;
_0x2060001:
	OUT  0x9,R30
; 0000 045D 
; 0000 045E }       // EndOf void clear_usart(void)
	RET
;
;
;/***************************************************************************************/
;	void 			timer_detik(char detik)
; 0000 0463 /***************************************************************************************
; 0000 0464 *	ABSTRAKSI  	: 	Menghitung nilai register TCNT1H dan TCNT1L dari input nilai
; 0000 0465 *				konstanta timer dalam satuan detik. Formula untuk menghitung
; 0000 0466 *				nilai register :
; 0000 0467 *				_TCNT1 = (TCNT1H << 8) + TCNT1L
; 0000 0468 *				_TCNT1 = (1 + 0xFFFF) - (konstanta_timer_detik * (sys_clock / prescaler))
; 0000 0469 *
; 0000 046A *      	INPUT		:	konstanta timer dalam satuan detik
; 0000 046B *	OUTPUT		:       tak ada
; 0000 046C *	RETURN		:       tak ada
; 0000 046D */
; 0000 046E {
_timer_detik:
; 0000 046F 	unsigned short _TCNT1;
; 0000 0470 
; 0000 0471         // hitung nilai vaiabel _TCNT1 dari nilai input berdasarkan formula :
; 0000 0472          	// _TCNT1 = (1 + 0xFFFF) - (konstanta_timer_detik * (sys_clock / prescaler))
; 0000 0473                 // menjadi bilangan 16 bit
; 0000 0474 	_TCNT1 = (1 + 0xFFFF) - (detik * 10800);
	RCALL __SAVELOCR2
;	detik -> Y+2
;	_TCNT1 -> R16,R17
	LDD  R26,Y+2
	LDI  R27,0
	LDI  R30,LOW(10800)
	LDI  R31,HIGH(10800)
	RCALL __MULW12U
	CLR  R22
	CLR  R23
	__GETD2N 0x10000
	RCALL __SWAPD12
	RCALL __SUBD12
	MOVW R16,R30
; 0000 0475 
; 0000 0476         // ambil 8 bit paling kanan dan jadikan nilai register TCNT1L
; 0000 0477         TCNT1L = _TCNT1 & 0xFF;
	MOV  R30,R16
	OUT  0x2C,R30
; 0000 0478 
; 0000 0479         // ambil 8 bit paling kiri dan jadikan nilai register TCNT1H
; 0000 047A         TCNT1H = _TCNT1 >> 8;
	MOV  R30,R17
	ANDI R31,HIGH(0x0)
	OUT  0x2D,R30
; 0000 047B 
; 0000 047C }       // EndOf void timer_detik(char detik)
	RCALL __LOADLOCR2
	ADIW R28,3
	RET
;
;
;/***************************************************************************************/
;	void main(void)
; 0000 0481 /***************************************************************************************
; 0000 0482 *
; 0000 0483 *	MAIN PROGRAM
; 0000 0484 *
; 0000 0485 */
; 0000 0486 {
_main:
; 0000 0487 	// pengaturan clock CPU dan menjaga agar kompatibel dengan versi code vision terdahulu
; 0000 0488 #pragma optsize-
; 0000 0489 	CLKPR=0x80;
	LDI  R30,LOW(128)
	OUT  0x26,R30
; 0000 048A 	CLKPR=0x00;
	LDI  R30,LOW(0)
	OUT  0x26,R30
; 0000 048B 	#ifdef _OPTIMIZE_SIZE_
; 0000 048C #pragma optsize+
; 0000 048D 	#endif
; 0000 048E 
; 0000 048F         // set bit register PORTB
; 0000 0490         PORTB=0x00;
	OUT  0x18,R30
; 0000 0491 
; 0000 0492         // set bit Data Direction Register PORTB
; 0000 0493 	DDRB=0xF8;
	LDI  R30,LOW(248)
	OUT  0x17,R30
; 0000 0494 
; 0000 0495         // set bit register PORTD
; 0000 0496         PORTD=0x09;
	LDI  R30,LOW(9)
	OUT  0x12,R30
; 0000 0497 
; 0000 0498         // set bit Data Direction Register PORTD
; 0000 0499 	DDRD=0x30;
	LDI  R30,LOW(48)
	OUT  0x11,R30
; 0000 049A 
; 0000 049B         // set register Analog Comparator
; 0000 049C         ACSR=0x80;
	LDI  R30,LOW(128)
	OUT  0x8,R30
; 0000 049D 
; 0000 049E         // set register EXT_IRQ_1 (External Interrupt 1 Request), Low Interrupt
; 0000 049F 	GIMSK=0x80;
	OUT  0x3B,R30
; 0000 04A0 	MCUCR=0x08;
	LDI  R30,LOW(8)
	OUT  0x35,R30
; 0000 04A1 	EIFR=0x80;
	LDI  R30,LOW(128)
	OUT  0x3A,R30
; 0000 04A2 
; 0000 04A3         // set register Timer 1, System clock 10.8kHz, Timer 1 overflow
; 0000 04A4 	TCCR1B=0x05;
	LDI  R30,LOW(5)
	OUT  0x2E,R30
; 0000 04A5 
; 0000 04A6         // set konstanta waktu 5 detik sebagai awalan
; 0000 04A7         timer_detik(INITIAL_TIME_C);
	ST   -Y,R30
	RCALL _timer_detik
; 0000 04A8 
; 0000 04A9         // set interupsi timer untuk Timer 1
; 0000 04AA         TIMSK=0x80;
	LDI  R30,LOW(128)
	OUT  0x39,R30
; 0000 04AB 
; 0000 04AC         // enable watchdog dan set nilai register untuk overflow :
; 0000 04AD // preprosesor direktif #pragma's
; 0000 04AE #pragma optsize-
; 0000 04AF 
; 0000 04B0 	/*=======================================================
; 0000 04B1         |                                         		|
; 0000 04B2         |	WATCHDOG OVERFLOW'S TIME CONSTANTS             	|
; 0000 04B3         |							|
; 0000 04B4         |-------------------------------------------------------|
; 0000 04B5         |	WDTCR	|	WDTCR	|	OVERFLOW	|
; 0000 04B6         |---------------|---------------|-----------------------|
; 0000 04B7         |	0x39	|	0x29	|	8 DETIK		|
; 0000 04B8         |	0x38	|	0x28	|	4 DETIK		|
; 0000 04B9         |	0x1F	|	0x0F	|	2 DETIK		|
; 0000 04BA         |	0x1E	|	0x0E	|	1 DETIK		|
; 0000 04BB         |-------------------------------------------------------|
; 0000 04BC         | Sumber : ATMEL datasheet for ATTINY2313               |
; 0000 04BD         ========================================================*/
; 0000 04BE 
; 0000 04BF         // set konstanta waktu watchdog overflow
; 0000 04C0         WDTCR=0x39;
	LDI  R30,LOW(57)
	OUT  0x21,R30
; 0000 04C1 	WDTCR=0x29;
	LDI  R30,LOW(41)
	OUT  0x21,R30
; 0000 04C2 	#ifdef _OPTIMIZE_SIZE_
; 0000 04C3 #pragma optsize+
; 0000 04C4 	#endif
; 0000 04C5 
; 0000 04C6         // indikator awalan hardware aktif :
; 0000 04C7         // nyalakan LED busy
; 0000 04C8         L_BUSY = 1;
	SBI  0x12,5
; 0000 04C9 
; 0000 04CA         // tunggu 500ms
; 0000 04CB         delay_ms(500);
	RCALL SUBOPT_0x3
; 0000 04CC 
; 0000 04CD         // nyalakan LED standby
; 0000 04CE         L_STBY = 1;
	SBI  0x12,4
; 0000 04CF 
; 0000 04D0         // tunggu 500ms
; 0000 04D1         delay_ms(500);
	RCALL SUBOPT_0x3
; 0000 04D2 
; 0000 04D3         // matikan LED busy
; 0000 04D4         L_BUSY = 0;
	CBI  0x12,5
; 0000 04D5 
; 0000 04D6         // tunggu 500ms
; 0000 04D7         delay_ms(500);
	RCALL SUBOPT_0x3
; 0000 04D8 
; 0000 04D9         // aktifkan interupsi global (berdasar setting register)
; 0000 04DA         #asm("sei")
	sei
; 0000 04DB 
; 0000 04DC         // tidak lakukan apapun selain menunggu interupsi timer1_ovf_isr
; 0000 04DD         while (1)
_0x7B:
; 0000 04DE         {
; 0000 04DF         	// blok ini kosong
; 0000 04E0         };
	RJMP _0x7B
; 0000 04E1 
; 0000 04E2 }	// END OF MAIN PROGRAM
_0x7E:
	RJMP _0x7E
;/*
;*
;*	END OF FILE
;*
;****************************************************************************************/
	#ifndef __SLEEP_DEFINED__
	#define __SLEEP_DEFINED__
	.EQU __se_bit=0x20
	.EQU __sm_mask=0x50
	.EQU __sm_powerdown=0x10
	.EQU __sm_standby=0x40
	.SET power_ctrl_reg=mcucr
	#endif

	.CSEG

	.CSEG

	.CSEG

	.DSEG
_rx_buffer:
	.BYTE 0x8
_posisi_lat:
	.BYTE 0x8
_posisi_long:
	.BYTE 0x9

	.ESEG
_beacon_stat:
	.DB  0x0

	.DSEG
_bit_stuff_G000:
	.BYTE 0x1
_crc_lo_S0000005000:
	.BYTE 0x1
_crc_hi_S0000005000:
	.BYTE 0x1
_xor_in_S0000007000:
	.BYTE 0x2
_buff_posisi_S000000C000:
	.BYTE 0x11

	.CSEG
;OPTIMIZER ADDED SUBROUTINE, CALLED 2 TIMES, CODE SIZE REDUCTION:11 WORDS
SUBOPT_0x0:
	ST   -Y,R0
	ST   -Y,R1
	ST   -Y,R15
	ST   -Y,R22
	ST   -Y,R23
	ST   -Y,R24
	ST   -Y,R25
	ST   -Y,R26
	ST   -Y,R27
	ST   -Y,R30
	ST   -Y,R31
	IN   R30,SREG
	ST   -Y,R30
	CBI  0x12,4
	RET

;OPTIMIZER ADDED SUBROUTINE, CALLED 5 TIMES, CODE SIZE REDUCTION:6 WORDS
SUBOPT_0x1:
	ST   -Y,R31
	ST   -Y,R30
	RJMP _delay_ms

;OPTIMIZER ADDED SUBROUTINE, CALLED 3 TIMES, CODE SIZE REDUCTION:2 WORDS
SUBOPT_0x2:
	LDI  R26,LOW(_beacon_stat)
	LDI  R27,HIGH(_beacon_stat)
	RCALL __EEPROMRDB
	RET

;OPTIMIZER ADDED SUBROUTINE, CALLED 4 TIMES, CODE SIZE REDUCTION:4 WORDS
SUBOPT_0x3:
	LDI  R30,LOW(500)
	LDI  R31,HIGH(500)
	RJMP SUBOPT_0x1

;OPTIMIZER ADDED SUBROUTINE, CALLED 15 TIMES, CODE SIZE REDUCTION:12 WORDS
SUBOPT_0x4:
	ST   -Y,R30
	RJMP _kirim_karakter

;OPTIMIZER ADDED SUBROUTINE, CALLED 3 TIMES, CODE SIZE REDUCTION:2 WORDS
SUBOPT_0x5:
	LDI  R30,LOW(0)
	STS  _bit_stuff_G000,R30
	RET

;OPTIMIZER ADDED SUBROUTINE, CALLED 7 TIMES, CODE SIZE REDUCTION:16 WORDS
SUBOPT_0x6:
	MOV  R30,R17
	LDI  R31,0
	RET

;OPTIMIZER ADDED SUBROUTINE, CALLED 4 TIMES, CODE SIZE REDUCTION:1 WORDS
SUBOPT_0x7:
	LPM  R30,Z
	RJMP SUBOPT_0x4

;OPTIMIZER ADDED SUBROUTINE, CALLED 3 TIMES, CODE SIZE REDUCTION:6 WORDS
SUBOPT_0x8:
	SUBI R30,LOW(-_matrix*2)
	SBCI R31,HIGH(-_matrix*2)
	LPM  R30,Z
	ST   -Y,R30
	RJMP _set_dac

;OPTIMIZER ADDED SUBROUTINE, CALLED 4 TIMES, CODE SIZE REDUCTION:1 WORDS
SUBOPT_0x9:
	__GETWRN 16,17,0
	RET

;OPTIMIZER ADDED SUBROUTINE, CALLED 4 TIMES, CODE SIZE REDUCTION:1 WORDS
SUBOPT_0xA:
	__ADDWRN 16,17,1
	RET

;OPTIMIZER ADDED SUBROUTINE, CALLED 2 TIMES, CODE SIZE REDUCTION:1 WORDS
SUBOPT_0xB:
	RCALL _getchar
	RCALL _getchar
	RCALL _getComma
	RJMP _getchar


	.CSEG
_delay_ms:
	ld   r30,y+
	ld   r31,y+
	adiw r30,0
	breq __delay_ms1
__delay_ms0:
	__DELAY_USW 0xACD
	wdr
	sbiw r30,1
	brne __delay_ms0
__delay_ms1:
	ret

__SUBD12:
	SUB  R30,R26
	SBC  R31,R27
	SBC  R22,R24
	SBC  R23,R25
	RET

__LSRB12:
	TST  R30
	MOV  R0,R30
	MOV  R30,R26
	BREQ __LSRB12R
__LSRB12L:
	LSR  R30
	DEC  R0
	BRNE __LSRB12L
__LSRB12R:
	RET

__MULW12U:
	MOV  R0,R26
	MOV  R1,R27
	LDI  R24,17
	CLR  R26
	SUB  R27,R27
	RJMP __MULW12U1
__MULW12U3:
	BRCC __MULW12U2
	ADD  R26,R0
	ADC  R27,R1
__MULW12U2:
	LSR  R27
	ROR  R26
__MULW12U1:
	ROR  R31
	ROR  R30
	DEC  R24
	BRNE __MULW12U3
	RET

__SWAPD12:
	MOV  R1,R24
	MOV  R24,R22
	MOV  R22,R1
	MOV  R1,R25
	MOV  R25,R23
	MOV  R23,R1

__SWAPW12:
	MOV  R1,R27
	MOV  R27,R31
	MOV  R31,R1

__SWAPB12:
	MOV  R1,R26
	MOV  R26,R30
	MOV  R30,R1
	RET

__EEPROMRDB:
	SBIC EECR,EEWE
	RJMP __EEPROMRDB
	PUSH R31
	IN   R31,SREG
	CLI
	OUT  EEARL,R26
	SBI  EECR,EERE
	IN   R30,EEDR
	OUT  SREG,R31
	POP  R31
	RET

__EEPROMWRB:
	SBIS EECR,EEWE
	RJMP __EEPROMWRB1
	WDR
	RJMP __EEPROMWRB
__EEPROMWRB1:
	IN   R25,SREG
	CLI
	OUT  EEARL,R26
	SBI  EECR,EERE
	IN   R24,EEDR
	CP   R30,R24
	BREQ __EEPROMWRB0
	OUT  EEDR,R30
	SBI  EECR,EEMWE
	SBI  EECR,EEWE
__EEPROMWRB0:
	OUT  SREG,R25
	RET

__SAVELOCR2:
	ST   -Y,R17
	ST   -Y,R16
	RET

__LOADLOCR2:
	LDD  R17,Y+1
	LD   R16,Y
	RET

__LOADLOCR2P:
	LD   R16,Y+
	LD   R17,Y+
	RET

;END OF CODE MARKER
__END_OF_CODE:
